{
	"segments": {
		"right": [
			{
				"function": "powerline.segments.common.sys.uptime",
				"priority": 50,
				"args": {
					"shorten_len": 1
				}
			},
			{
				"function": "custom.battery",
				"args": {
					"max_percent": 95
				}
			},
			{
				"function": "custom.system_load",
				"priority": 50,
				"args": {
					"num_avgs": 1
				}
			},
			{
				"function": "powerline.segments.common.time.date",
				"args": {
					"format": "%m/%d %H:%M",
					"istime": true
				}
			},
			{
				"function": "powerline.segments.common.net.hostname",
				"args": {
					"exclude_domain": true
				}
			}
		]
	}
}
